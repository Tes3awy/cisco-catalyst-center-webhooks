{% macro render_pagination(pagination) %}
<ul class="pagination m-0 ms-auto">
    {% for page in pagination.iter_pages(left_edge=1, right_edge=1, left_current=1, right_current=2) %}
    {% if page %}
    {% if page != pagination.page %}
    <li class="page-item">
        <a class="page-link" href="{{ url_for('main.index', page=page) }}">{{ page }}</a>
    </li>
    {% else %}
    <li class="page-item active">
        <a class="page-link" href="javascript:">{{ page }}</a>
    </li>
    {% endif %}
    {% else %}
    <li class="page-item disabled">
        <a class="page-link">â€¦</a>
    </li>
    {% endif %}
    {% endfor %}
</ul>
{% endmacro %}